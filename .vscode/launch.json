    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug benchmark 'benchmark'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bench=benchmark",
          "--package=hdmifiletransporter"
        ],
        "filter": {
          "name": "benchmark",
          "kind": "bench"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Tool 'colorframe'",
      "cargo": {
        "args": [
          "build",
          "--bin=colorframe",
          "--package=hdmifiletransporter"
        ],
        "filter": {
          "name": "colorframe",
          "kind": "bin"
        }
      },
      "args": [
        
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}
