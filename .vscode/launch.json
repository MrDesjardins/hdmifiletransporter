  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Unit tests library",
      "cargo": {
        "args": ["test", "--no-run", "--lib", "--package=hdmifiletransporter"],
        "filter": {
          "name": "hdmifiletransporter",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Inject 'hdmifiletransporter'",
      "cargo": {
        "args": [
          "build",
          "--bin=hdmifiletransporter",
          "--package=hdmifiletransporter"
        ],
        "filter": {
          "name": "hdmifiletransporter",
          "kind": "bin"
        }
      },
      "args": [
        "-m",
        "inject",
        "-i",
        "testAssets/text2.txt",
        "-o",
        "outputs/out1.mp4",
        "--fps",
        "30",
        "--height",
        "1080",
        "--width",
        "1920",
        "--size",
        "1",
        "-a",
        "bw"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Extract 'hdmifiletransporter'",
      "cargo": {
        "args": [
          "build",
          "--bin=hdmifiletransporter",
          "--package=hdmifiletransporter"
        ],
        "filter": {
          "name": "hdmifiletransporter",
          "kind": "bin"
        }
      },
      "args": [
        "-m",
        "extract",
        "-i",
        "outputs/out1.mp4",
        "-o",
        "outputs/text2.txt",
        "--fps",
        "30",
        "--height",
        "1080",
        "--width",
        "1920",
        "--size",
        "10",
        "-a",
        "bw",
        "-p",
        "true"
      ],
      "cwd": "${workspaceFolder}"
    },
    // {
